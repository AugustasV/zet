{
   "name": "Go with Keg",
   "build": {
      "dockerfile": "Dockerfile"
   },
   "customizations": {      
      "vscode": {
         "extensions": [
            "golang.go",
            "ms-azuretools.vscode-docker"
         ]   
      }
   },
   "containerEnv": {
     "GIT_USER_EMAIL":      "${localEnv:GIT_USER_EMAIL}",
     "GIT_USER_NAME":       "${localEnv:GIT_USER_NAME}"
   }
}